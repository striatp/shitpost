{
  "$schema": "https://registry.powershell-dock.com/schema/dock.json",
  "version": "0.0.1",
  "registry": "https://registry.powershell-dock.com/",
  "mirrors": [],
  "paths": {
    "bin": "./bin",
    "cache": "./cache",
    "logs": "./logs",
    "modules": "./modules"
  },
  "settings": {
    "autoUpdate": true,
    "checkIntegrity": true,
    "parallelInstalls": 3,
    "telemetry": false
  },
  "cache": {
    "enabled": true,
    "maxSizeMB": 50,
    "ttlDays": 30
  }
}
