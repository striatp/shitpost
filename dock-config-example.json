{
	"$schema": "http://localhost:3000/schema/dock.json",
	"version": "0.0.1",
	"registry": "https://registry.powershell-dock.com/",
	"mirrors": [],
	"paths": {
		"bin": "./bin",
		"cache": "./cache",
		"logs": "./logs",
		"modules": "./modules"
	},
	"settings": {
		"cache": {
			"enabled": true,
			"maxSizeMB": 50,
			"ttlDays": 30
		},
		"maxLogs": 10,
		"autoUpdate": true,
		"checkIntegrity": true
	}
}
