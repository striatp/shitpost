{
  "version": "1.0.0",
  "registry": {
    "default": "https://registry.dock.sh/",
    "mirrors": [
      "https://mirror1.dock.sh/",
      "https://mirror2.dock.sh/"
    ],
    "auth": {
      "https://registry.dock.sh/": {
        "token": "ENCRYPTED_TOKEN_HERE"
      }
    }
  },
  "settings": {
    "autoUpdate": true,
    "checkIntegrity": true,
    "parallelInstalls": 3,
    "logLevel": "info",
    "colors": true
  },
  "cache": {
    "enabled": true,
    "maxSizeMB": 512,
    "ttlDays": 30
  },
  "paths": {
    "bin": "~/.dock/bin",
    "logs": "~/.dock/logs",
    "cache": "~/.dock/cache",
    "modules": "~/.dock/modules"
  }
}
